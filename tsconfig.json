{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@static/*": [
        "./static/images/*"
      ]
    },
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "experimentalDecorators": true
  },
  "include": [
    "./src/**/*",
    "./static/**/*", "src/routes/mkt/mobx/.js"
  ]
}